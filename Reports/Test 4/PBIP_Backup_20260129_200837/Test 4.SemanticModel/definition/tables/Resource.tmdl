	partition Resource = m
		mode: directQuery
		source =
				let
				    Dataverse = CommonDataService.Database(DataverseURL,[CreateNavigationProperties=false]),
				    Source = Value.NativeQuery(Dataverse,"
				
				    -- View Filter: Active Resources
				
				    SELECT Base.cai_alias
				        ,Base.cai_displayname
				        ,Base.cai_managerresourceid
				        ,Base.cai_managerresourceidname
				        ,Base.cai_manageruserid
				        ,Base.cai_manageruseridname
				        ,Base.ownerid
				        ,Base.owneridname
				        ,Base.cai_positionid
				        ,Base.cai_positionidname
				        ,Base.tov2_reports_to_level1
				        ,Base.tov2_reports_to_level2
				        ,Base.tov2_reports_to_level3
				        ,Base.tov2_reports_to_level4
				        ,Base.tov2_reports_to_level5
				        ,Base.tov2_reports_to_level6
				        ,Base.tov2_reports_to_level7
				        ,Base.cai_reports_to_manager
				        ,Base.cai_resourceid
				        ,Base.cai_resourcelabel
				        ,Base.cai_rto_exceptionnote
				        ,Base.cai_rto_locationid
				        ,Base.cai_rto_locationidname
				        ,Base.cai_rto_planneddate
				        ,Base.cai_rto_statusname
				        ,Base.cai_userid
				        ,Base.cai_useridname
				    FROM cai_Resource as Base
				    WHERE (Base.statecode = 0 AND Base.cai_managesdirect > 0)
				
				    " ,null ,[EnableFolding=true])
				in
				    Source

	annotation PBI_NavigationStepName = Navigation

	annotation PBI_ResultType = Table

