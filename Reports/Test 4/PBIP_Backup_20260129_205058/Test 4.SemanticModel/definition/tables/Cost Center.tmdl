	partition 'Cost Center' = m
		mode: directQuery
		source =
				let
				    Dataverse = CommonDataService.Database(DataverseURL,[CreateNavigationProperties=false]),
				    Source = Value.NativeQuery(Dataverse,"
				
				    -- View Filter: Active Cost Centers
				
				    SELECT Base.tov2_companycode
				        ,Base.tov2_companycodename
				        ,Base.tov2_costcenter
				        ,Base.tov2_costcenterid
				        ,Base.tov2_costcenterdescription
				        ,Base.tov2_hasheadcountname
				        ,Base.tov2_latestheadcountdate
				        ,Base.ownerid
				        ,Base.owneridname
				        ,Base.owningbusinessunit
				        ,Base.owningbusinessunitname
				        ,Base.tov2_profit_center
				        ,Base.tov2_profitcenterdescription
				        ,Base.tov2_secfunctionalarea
				    FROM tov2_CostCenter as Base
				    WHERE (Base.statecode = 0)
				
				    " ,null ,[EnableFolding=true])
				in
				    Source

