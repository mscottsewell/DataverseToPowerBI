	partition 'Service or Initiative' = m
		mode: directQuery
		source =
				let
				    Dataverse = CommonDataService.Database(DataverseURL,[CreateNavigationProperties=false]),
				    Source = Value.NativeQuery(Dataverse,"
				
				    -- View Filter: Active Services & Initiatives
				
				    SELECT Base.cai_description
				        ,Base.cai_docslink
				        ,Base.cai_feedbacklink
				        ,Base.cai_level
				        ,Base.cai_level_1
				        ,Base.cai_level_2
				        ,Base.cai_level_3
				        ,Base.cai_level_4
				        ,Base.cai_level_5
				        ,Base.cai_level_6
				        ,Base.cai_level_7
				        ,Base.cai_level_8
				        ,Base.cai_level_9
				        ,Base.cai_hierarchypath
				        ,Base.cai_name
				        ,Base.ownerid
				        ,Base.owneridname
				        ,Base.cai_area
				        ,Base.cai_areaname
				        ,Base.cai_parentserviceorinitiativeid
				        ,Base.cai_parentserviceorinitiativeidname
				        ,Base.cai_repolink
				        ,Base.cai_serviceorinitiativeid
				    FROM cai_ServiceorInitiative as Base
				    WHERE (Base.statecode = 0)
				
				    " ,null ,[EnableFolding=true])
				in
				    Source

