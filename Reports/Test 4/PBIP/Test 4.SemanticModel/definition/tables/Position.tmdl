	partition Position = m
		mode: directQuery
		source =
				let
				    Dataverse = CommonDataService.Database(DataverseURL,[CreateNavigationProperties=false]),
				    Source = Value.NativeQuery(Dataverse,"
				
				    -- View Filter: Active Positions
				
				    SELECT Base.tov2_alias
				        ,Base.tov2_city
				        ,Base.tov2_companycode
				        ,Base.tov2_companycodename
				        ,Base.tov2_costcenter
				        ,Base.tov2_costcentername
				        ,Base.tov2_employee_name
				        ,Base.tov2_location
				        ,Base.ownerid
				        ,Base.owneridname
				        ,Base.owningbusinessunit
				        ,Base.owningbusinessunitname
				        ,Base.tov2_positionid
				        ,Base.tov2_positionnumber
				        ,Base.tov2_positionstatus
				        ,Base.tov2_position_subtype
				        ,Base.tov2_position_type
				        ,Base.tov2_profession
				        ,Base.tov2_project_code
				        ,Base.tov2_project_description
				        ,Base.tov2_project_name
				        ,Base.tov2_reports_to_level1
				        ,Base.tov2_reports_to_level2
				        ,Base.tov2_reports_to_level3
				        ,Base.tov2_reports_to_level4
				        ,Base.tov2_reports_to_level5
				        ,Base.tov2_reports_to_level6
				        ,Base.tov2_reports_to_level7
				        ,Base.tov2_reports_to_manager
				        ,Base.tov2_requisitionnumbers
				        ,Base.tov2_sec_functional_area
				        ,Base.tov2_standard_title
				    FROM tov2_Position as Base
				    WHERE (Base.statecode = 0)
				
				    " ,null ,[EnableFolding=true])
				in
				    Source

	annotation PBI_NavigationStepName = Navigation

